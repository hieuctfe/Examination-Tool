@import "../../../../assets/css/theme";

.card{
  color: $main-cl;
  cursor: pointer;
  &:hover{
    box-shadow: 0 3px 1px -2px rgba(0,0,0,.2), 0 2px 2px 0 rgba(0,0,0,.14), 0 1px 5px 0 rgba(0,0,0,.12);
  }
}

.pagination{
  .page-link{
    color: $main-cl;
  }
  .active{
    background: $main-cl;
    color: white;
  }
}
